<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.chrav.emolyser.QuesTen">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="346dp"
        android:layout_height="96dp"
        android:layout_marginLeft="16dp"
        android:text="Question Ten \nWhich of the following caught your attention?"
        android:textStyle="bold"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <Button
        android:id="@+id/button19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="Back"
        android:textColor="#000000"
        android:layout_marginLeft="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        tools:layout_editor_absoluteX="31dp" />

    <Button
        android:id="@+id/button20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="16dp"
        android:text="Next"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="352dp"
        android:layout_height="293dp"
        android:layout_marginLeft="29dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/pic5"
        app:layout_constraintBottom_toTopOf="@+id/button20"
        android:layout_marginBottom="20dp" />

    <RadioGroup
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:weightSum="1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.796">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteY="327dp">

            <RadioButton
                android:id="@+id/radioButton33"
                android:layout_width="103dp"
                android:layout_height="52dp"
                android:layout_marginLeft="20dp"
                android:layout_weight="0.26"
                android:text="Adventure"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintHorizontal_bias="0.119"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                tools:layout_editor_absoluteY="203dp" />

            <RadioButton
                android:id="@+id/radioButton34"
                android:layout_width="94dp"
                android:layout_height="53dp"
                android:layout_marginLeft="10dp"
                android:layout_weight="0.65"
                android:text="Career"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintHorizontal_bias="0.098"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                tools:layout_editor_absoluteY="266dp" />

            <RadioButton
                android:id="@+id/radioButton35"
                android:layout_width="137dp"
                android:layout_height="60dp"
                android:layout_marginLeft="25dp"
                android:text="Happiness"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintHorizontal_bias="0.098"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                tools:layout_editor_absoluteY="266dp" />
        </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="374dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent">

        <RadioButton
        android:id="@+id/radioButton36"
        android:layout_width="94dp"
        android:layout_height="53dp"
        android:layout_marginLeft="20dp"
        android:layout_weight="0.09"
        android:text="Health"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.098"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="266dp" />

    <RadioButton
        android:id="@+id/radioButton37"
        android:layout_width="118dp"
        android:layout_height="44dp"
        android:layout_marginLeft="20dp"
        android:text="Knowledge"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.098"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="266dp" />

        <RadioButton
            android:id="@+id/radioButton38"
            android:layout_width="94dp"
            android:layout_height="53dp"
            android:layout_weight="0.27"
            android:text="Money"
            android:textColor="#000000"
            android:textStyle="bold"
            app:layout_constraintHorizontal_bias="0.098"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteY="266dp" />
    </LinearLayout>
    </RadioGroup>


</android.support.constraint.ConstraintLayout>
