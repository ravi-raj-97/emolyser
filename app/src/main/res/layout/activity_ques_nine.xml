<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.chrav.emolyser.QuesNIne">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="289dp"
        android:layout_height="79dp"
        android:layout_marginLeft="16dp"
        android:text="Question Nine \nIn your opinion, looking at this picture:"
        android:textStyle="bold"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="Back"
        android:textColor="#000000"
        android:layout_marginLeft="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        tools:layout_editor_absoluteX="31dp" />

    <Button
        android:id="@+id/button18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="16dp"
        android:text="Next"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <RadioGroup
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="362dp"
        app:layout_constraintLeft_toRightOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        android:weightSum="1"
        app:layout_constraintHorizontal_bias="0.0">

        <RadioButton
            android:id="@+id/radioButton31"
            android:layout_width="103dp"
            android:layout_height="80dp"
            android:layout_marginLeft="30dp"
            android:text="Is the boy just different and glad about it?"
            android:textColor="#000000"
            android:textStyle="bold"
            android:textSize="15dp"
            app:layout_constraintHorizontal_bias="0.119"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteY="203dp"
            android:layout_weight="0.15" />

        <RadioButton
            android:id="@+id/radioButton32"
            android:layout_width="94dp"
            android:layout_height="match_parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_weight="0.16"
            android:text="Is the boy missing being in the group?"
            android:textColor="#000000"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintHorizontal_bias="0.098"
            app:layout_constraintLeft_toLeftOf="parent"
            tools:layout_editor_absoluteY="266dp" />

    </RadioGroup>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="352dp"
        android:layout_height="293dp"
        android:layout_marginLeft="29dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/pic4"
        app:layout_constraintBottom_toTopOf="@+id/button18"
        android:layout_marginBottom="22dp" />

</android.support.constraint.ConstraintLayout>
